<mvc:View controllerName="lists.controller.App"
    xmlns="sap.m"
    xmlns:core="sap.ui.core"
    xmlns:layout="sap.ui.layout"
    xmlns:mvc="sap.ui.core.mvc" 
    displayBlock="true">
      
      <Shell >
        <App >
           <pages>
              <Page id="page" 
                    title="{i18n>title}">
                <content >
                  <IconTabBar>
                    <items>
                       <IconTabFilter text="{i18n>objectList}">
                          <List items="{/Products}">
                             <items>
                               <ObjectListItem title="{Material} {Brand} {Model}"
                                               number="{Price}"
                                               numberUnit="{Currency}"
                                               type="Active"/>
                             </items>
                          </List>
                       </IconTabFilter>
                       <IconTabFilter text="{i18n>standardList}">
                       
                       </IconTabFilter>
                       <IconTabFilter text="{i18n>inputList}">
                       
                       </IconTabFilter>
                       <IconTabFilter text="{i18n>notificationList}">
                       
                       </IconTabFilter>
                       <IconTabFilter text="{i18n>customList}">
                       
                       </IconTabFilter>
                    </items>
                  </IconTabBar>  
                </content>
            </Page>
           </pages>
        </App>
      </Shell>

</mvc:View>
